    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "husky": "^5.1.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.6"
  }
